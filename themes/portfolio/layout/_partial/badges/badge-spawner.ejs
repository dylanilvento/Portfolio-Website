<% page.tags.each(function(tag) { %>

		<% if(tag.name == "games") { %>
			<%- partial('_partial/badges/games-badge', {}, {cache: true}) %>

		<% } else if(tag.name == "web") { %>
			<%- partial('_partial/badges/web-badge', {}, {cache: true}) %>

		<% } %>


<% }) %>

